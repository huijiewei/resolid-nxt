{
  "name": "@resolid/nxt-run",
  "version": "1.0.0",
  "description": "Get your app up and running with React",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./server": {
      "browser": {
        "types": "./dist/browser.d.ts",
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      },
      "default": {
        "types": "./dist/server.d.ts",
        "import": "./dist/server.js",
        "require": "./dist/server.cjs"
      }
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "node": [
        "./dist/node.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "dependencies": {
    "cookie": "^0.5.0",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/parser": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@remix-run/router": "^1.6.3",
    "@types/babel__core": "^7.20.1",
    "@types/babel__traverse": "^7.20.1",
    "@types/cookie": "^0.5.1",
    "@types/set-cookie-parser": "^2.4.2",
    "@vitejs/plugin-react": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.13.0",
    "rollup": "^3.25.1",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.28"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "@babel/parser": ">=7",
    "@babel/traverse": ">=7",
    "@vitejs/plugin-react": ">=4",
    "react": ">=18",
    "react-dom": ">=18",
    "react-helmet-async": ">=1",
    "react-router-dom": ">=6",
    "vite": ">=4",
    "vite-plugin-inspect": ">=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/resolid-nxt",
    "directory": "packages/nxt-run"
  },
  "homepage": "https://nxt.resolid.tech/run",
  "license": "MIT"
}
