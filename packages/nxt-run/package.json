{
  "name": "@resolid/nxt-run",
  "version": "1.0.0",
  "description": "Get your app up and running with React",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./server": {
      "browser": {
        "import": {
          "types": "./dist/browser.d.ts",
          "default": "./dist/browser.js"
        },
        "require": {
          "types": "./dist/browser.d.cts",
          "default": "./dist/browser.cjs"
        }
      },
      "default": {
        "import": {
          "types": "./dist/server.d.ts",
          "default": "./dist/server.js"
        },
        "require": {
          "types": "./dist/server.d.cts",
          "default": "./dist/server.cjs"
        }
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "node": [
        "./dist/node.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "dependencies": {
    "cookie": "^0.5.0",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@babel/traverse": "^7.22.8",
    "@remix-run/router": "^1.7.1",
    "@types/babel__core": "^7.20.1",
    "@types/babel__traverse": "^7.20.1",
    "@types/cookie": "^0.5.1",
    "@types/set-cookie-parser": "^2.4.2",
    "@vitejs/plugin-react": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.14.1",
    "rollup": "^3.26.2",
    "vite": "^4.4.4",
    "vite-plugin-inspect": "^0.7.33"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "@babel/parser": ">=7",
    "@babel/traverse": ">=7",
    "@vitejs/plugin-react": ">=4",
    "react": ">=18",
    "react-dom": ">=18",
    "react-helmet-async": ">=1",
    "react-router-dom": ">=6",
    "vite": ">=4",
    "vite-plugin-inspect": ">=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/resolid-nxt",
    "directory": "packages/nxt-run"
  },
  "homepage": "https://nxt.resolid.tech/run",
  "license": "MIT"
}
