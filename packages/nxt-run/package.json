{
  "name": "@resolid/nxt-run",
  "version": "1.0.0",
  "description": "Get your app up and running with React",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "types": "./dist/node.d.ts"
    },
    "./server": {
      "browser": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs",
        "types": "./dist/browser.d.ts"
      },
      "node": {
        "import": "./dist/server.js",
        "require": "./dist/server.cjs",
        "types": "./dist/server.d.ts"
      }
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs",
      "types": "./dist/vite.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "node": [
        "./dist/node.d.ts"
      ],
      "server": [
        "./dist/server.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/parser": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.18.3",
    "@types/set-cookie-parser": "^2.4.2",
    "@vitejs/plugin-react": "^3.1.0",
    "node-html-parser": "^6.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.10.0",
    "rollup": "^3.20.2",
    "set-cookie-parser": "^2.6.0",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.21"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "@babel/parser": ">=7",
    "@babel/traverse": ">=7",
    "@vitejs/plugin-react": ">=3",
    "node-html-parser": ">=6",
    "react": ">=18",
    "react-dom": ">=18",
    "react-helmet-async": ">=1",
    "react-router-dom": ">=6",
    "set-cookie-parser": ">=2",
    "vite": ">=4",
    "vite-plugin-inspect": ">=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resolid/nxt",
    "directory": "packages/nxt-run"
  },
  "homepage": "https://nxt.resolid.tech/run",
  "license": "MIT"
}
