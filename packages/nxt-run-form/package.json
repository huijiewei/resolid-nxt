{
  "name": "@resolid/nxt-run-form",
  "version": "1.0.0",
  "description": "Utility wrapper around react-hook-form for use with Resolid Nxt",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@resolid/nxt-utils": "workspace:*"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@resolid/nxt-run": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.1",
    "react-router-dom": "^6.20.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@resolid/nxt-run": "*",
    "react": ">=18",
    "react-dom": ">=18",
    "react-hook-form": ">=7",
    "react-router-dom": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/resolid-nxt",
    "directory": "packages/nxt-run-form"
  },
  "homepage": "https://nxt.resolid.tech",
  "license": "MIT"
}
