{
  "path": "/button/Button.tsx",
  "props": [
    {
      "name": "size",
      "type": "\"xs\" | \"sm\" | \"md\" | \"lg\" | \"xl\"",
      "required": false,
      "description": "Size",
      "defaultValue": "'md'"
    },
    {
      "name": "color",
      "type": "\"primary\" | \"neutral\" | \"success\" | \"warning\" | \"danger\"",
      "required": false,
      "description": "Color",
      "defaultValue": "'primary'"
    },
    {
      "name": "variant",
      "type": "\"link\" | \"solid\" | \"outline\" | \"light\" | \"subtle\"",
      "required": false,
      "description": "Variant",
      "defaultValue": "'solid'"
    },
    { "name": "disabled", "type": "boolean", "required": false, "description": "Disabled", "defaultValue": "false" },
    {
      "name": "type",
      "type": "\"button\" | \"submit\" | \"reset\"",
      "required": false,
      "description": "Html Type",
      "defaultValue": "'button'"
    },
    { "name": "active", "type": "boolean", "required": false, "description": "Active", "defaultValue": "false" },
    { "name": "loading", "type": "boolean", "required": false, "description": "Loading", "defaultValue": "false" },
    { "name": "loadingText", "type": "string", "required": false, "description": "Loading text", "defaultValue": "" },
    { "name": "fullWidth", "type": "boolean", "required": false, "description": "Full Width", "defaultValue": "false" },
    { "name": "spinner", "type": "Element", "required": false, "description": "Spinner", "defaultValue": "" },
    {
      "name": "spinnerPlacement",
      "type": "\"start\" | \"end\"",
      "required": false,
      "description": "Spinner Placement",
      "defaultValue": "'start'"
    }
  ]
}
