{
  "path": "/radio/RadioGroup.tsx",
  "props": [
    {
      "name": "size",
      "type": "\"xs\" | \"sm\" | \"md\" | \"lg\" | \"xl\"",
      "required": false,
      "description": "Size",
      "defaultValue": "'md'"
    },
    {
      "name": "color",
      "type": "\"primary\" | \"neutral\" | \"success\" | \"warning\" | \"danger\"",
      "required": false,
      "description": "Color",
      "defaultValue": "'primary'"
    },
    { "name": "disabled", "type": "boolean", "required": false, "description": "Disabled", "defaultValue": "false" },
    {
      "name": "name",
      "type": "string",
      "required": false,
      "description": "The name of the input field in a radio",
      "defaultValue": ""
    },
    { "name": "value", "type": "string | number", "required": false, "description": "Value", "defaultValue": "" },
    {
      "name": "defaultValue",
      "type": "string | number",
      "required": false,
      "description": "Default value",
      "defaultValue": ""
    },
    {
      "name": "onChange",
      "type": "((value: string | number) => void)",
      "required": false,
      "description": "onChange callback",
      "defaultValue": ""
    }
  ]
}
